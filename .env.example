# Gemini API 认证（必需）
SECURE_1PSID=
SECURE_1PSIDTS=

# 服务器配置
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Gemini API 配置
GEMINI_PROXY=
GEMINI_TIMEOUT=300
GEMINI_AUTO_REFRESH=true

# CORS 配置
CORS_ORIGINS=*
CORS_METHODS=*
CORS_HEADERS=*

# 日志配置
LOG_LEVEL=INFO

# API 信息
API_TITLE=Gemini API Wrapper
API_VERSION=1.0.0
API_DESCRIPTION=OpenAI-compatible API wrapper for Google Gemini

# 速率限制（可选）
RATE_LIMIT_ENABLED=false
RATE_LIMIT_REQUESTS=60
RATE_LIMIT_WINDOW=60